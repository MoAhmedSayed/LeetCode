class Solution {
public:
    bool isPalindrome(int x) {
        int tempX = x;
        if (x < 0 || x > INT_MAX)
            return false;
        stack<int> numbers;
        while (x != 0) {
            numbers.push(x % 10);
            x /= 10;
        }
        int power = 0;
        int newX = 0;
        while (!numbers.empty()) {
            if (numbers.top() * pow(10, power) + newX > INT_MAX)
                return false;

            newX += numbers.top() * pow(10, power);
1
